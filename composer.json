{
    "name": "helhum/ter-client",
    "description": "Client for interacting with the official TYPO3 Extension Repository (TER). Contains API Symfony Commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Helhum\\TerClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helhum\\TerClient\\Tests\\Unit\\": "tests/Unit/"
        }
    },
    "require": {
        "php": ">5.3.3 < 7.2",
        "symfony/console": "^2.7 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mikey179/vfsStream": "~1.3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "bin": [
        "ter-client"
    ]
}
